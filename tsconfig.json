{
  "files": [
    "./src/routes/healthCheck.ts",
    "./src/services/databaseService.ts"
  ],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
